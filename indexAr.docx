      fallbackLng: 'en', // Fallback to English if a translation is missing
      interpolation: {
        escapeValue: false // Allows HTML tags in translations
      }
    });

    document.addEventListener('DOMContentLoaded', () => {
      const toggleButton = document.getElementById('toggleLanguage');

      // Handle language toggle button click event
      toggleButton.addEventListener('click', () => {
        const currentLanguage = i18next.language;

        // Determine the language to switch to
        const newLanguage = currentLanguage === 'en' ? 'en' : 'en';

        // Update direction for Arabic language
        if (newLanguage === 'ar') {
          window.location.href = 'indexAr.html'
          document.body.dir = 'rtl'; // Set the direction to right-to-left (RTL)
        } else {
          window.location.href = 'index.html'

          document.body.dir = 'ltr'; // Set the direction to left-to-right (LTR)
        }

        // Change the language and update the translations
        i18next.changeLanguage(newLanguage, (err, t) => {
          if (err) return console.error('Error loading translation:', err);

          // Translate all elements with a 'translate' attribute
          const elements = document.querySelectorAll('[translate]');
          elements.forEach(element => {
            const key = element.getAttribute('translate');
            element.textContent = t(key); // Replace the text with translation
          });
        });
      });

      // Initial translation when the page loads
      const elements = document.querySelectorAll('[translate]');
      elements.forEach(element => {
        const key = element.getAttribute('translate');
        element.textContent = i18next.t(key); // Replace the text with translation
      });
    });

    document.addEventListener('DOMContentLoaded', () => {
      // Translate all elements with a 'translate' attribute
      const elements = document.querySelectorAll('[translate]');
      elements.forEach(element => {
        const key = element.getAttribute('translate');
        element.textContent = i18next.t(key); // Replace the text with translation
      });
    });
  </script>
</body>

</html>
